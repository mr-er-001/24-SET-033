using System;
using System.Collections.Generic;

namespace SemesterSubjectsApp
{
    class Program
    {
        static void Main(string[] args)
        {
            List<string> secondSemesterSubjects = new List<string>()
            {
                "English-II",
                "Pakistan Studies",
                "Object-Oriented Programming",
                "Database Systems",
                "Waves and Oscillation (Physics)",
                "Mathematics-II"
            };

            Console.WriteLine("2nd Semester Subjects for ICS (Physics Group):\n");

            foreach (string subject in secondSemesterSubjects)
            {
                Console.WriteLine("- " + subject);
            }

            Console.WriteLine("\nPress any key to exit...");
            Console.ReadKey();
        }
    }
}
